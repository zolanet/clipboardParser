{
  "name": "clipboard-parser",
  "displayName": "Clipboard Parser",
  "description": "Parse clipboard data before pasting",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clipboard-parser.pasteEtcsFilenames",
        "title": "Paste ETCS Filenames"
      },
      {
        "command": "clipboard-parser.pasteAsMdUri",
        "title": "Paste as Markdown URI"
      },
      {
        "command": "clipboard-parser.pasteComparatorReport",
        "title": "Paste Comparator Report"
      },
      {
        "command": "clipboard-parser.prettyPasteStackTrace",
        "title": "Pretty Paste Stack Trace"
      }
    ]
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/zolanet/clipboardParser.git"
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
